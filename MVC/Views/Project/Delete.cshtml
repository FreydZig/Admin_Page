@model Project

<form method="post" asp-action="DeletePOST">
	<input asp-for="Id" hidden />
	<div class="border p-3 mt-4">
		<div class="row pb-2">
			<h2 class="text-primary">Delete Project</h2>
			<hr />
		</div>
		@*<div asp-validation-summary="All"></div>*@
		<div class="mb-3">
			<label asp-for="Name"></label>
			<div>
				<input asp-for="Name" disabled class="from-control"/>
			</div>
		</div>
		<div class="mb-3">
			<label>Company Customer</label>
			<div>
				<input asp-for="Name_company_cust" disabled class="from-control"/>
			</div>
		</div>
		<div class="mb-3">
			<label>Company Executor</label>
			<div>
				<input asp-for="Name_company_exe" disabled class="from-control"/>
			</div>
		</div>
		<div class="mb-3">
			<label>Director</label>
			<div>
				<input asp-for="Name_director" disabled class="from-control"/>
			</div>
		</div>
		<div class="mb-3">
			<label>Date Start</label>
			<div>
				<input asp-for="Date_start" disabled class="from-control"/>
			</div>
		</div>
		<div class="mb-3">
			<label>Date End</label>
			<div>
				<input asp-for="Date_end" disabled class="from-control"/>
			</div>
		</div>
		<div class="mb-3">
			<label>Priority</label>
			<div>
				<input asp-for="Priority" disabled class="from-control"/>
			</div>
		</div>
		<button type="submit" class="btn btn-outline-danger" style="width:150px">Delete</button>
		<a asp-controller="Project" asp-action="Index" class="btn btn-outline-warning" style="width:150px">
			Back
		</a>
	</div>
</form>

@section Scripts{
	@{
		<partial name="_ValidationScriptsPartial"/>
	}
}